{
    "chmod": "0755",
    "directories": ["src"],
    "finder": [
    {
        "name": "*.php",
        "exclude": [
            "kherge",
            "mockery",
            "pdepend",
            "php_codesniffer",
            "phpmd",
            "phpseclib",
            "phpunit",
            "satooshi",
            "Tests",
            "tests",
            "test"
        ],
        "in": "vendor"
    }
],
    "main": "bin/transmission-cli",
    "output": "build/transmission-cli.phar",
    "stub": true
}