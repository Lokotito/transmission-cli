{
    "name": "popstas/transmission-cli",
    "description": "Transmission client for automate weburg.net downloads",
    "require": {
        "php": ">=5.6",
        "symfony/console": "~2.1",
        "symfony/yaml": "~3.0",
        "martial/transmission-api": "~1.0",
        "influxdb/influxdb-php": "~1.1",
        "stecman/symfony-console-completion": "~0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.1.3",
        "mockery/mockery": "~0.9.4",
        "satooshi/php-coveralls": "2.0.x-dev",
        "kherge/box": "~2.6",
        "phpmd/phpmd": "~2.3",
        "squizlabs/php_codesniffer": "2.*",
        "brianium/paratest": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Popstas\\Transmission\\Console\\": "src/",
            "Popstas\\Transmission\\Console\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/transmission-cli"]
}
